<body class="chat">
<!-- Top header Links Starts Here -->
<%=render :partial=>'/layouts/header_top'%>
<!-- Top header Links Ends Here -->
<div class="container">
	<div class="width-wrapper">
		<div id="chat" class="left-col main-content">
			<!--Header Navigation Tabs -->
			<%= render :partial => '/layouts/header_nav_tabs' %>
			
			<div class="box-top-left"></div>
			<div class="box-top"></div>
			<div class="box-top-right"></div>
			<div class="main-padder">
			<form id="chat-form" action="#">
				<div class="input-wrapper textarea-wrapper">
				<div class="box-top-left"></div>
				<div class="box-top"></div>
				<div class="box-top-right"></div>
				<div class="box-body">
					<textarea id="chat-msg" name="chat-msg" rows="3" cols="50">Type your chat message here&hellip;</textarea>
				</div>
				
				<div class="box-bottom-left"></div>
				<div class="box-bottom"></div>
				<div class="box-bottom-right"></div>
				</div>

				<div class="submit-actions">
					<p class="command-alt">enter/return</p>
					<input id="submit-chat" class="btn send-btn" type="submit" value="Send" />
				</div>
			</form>

				<ul id="extra-actions">
					<li class="modal-trigger">
						<a class="upload-files" href="#">Upload Files</a>
									<div id="add-file-modal" class="modal">
				<a href="#" class="btn close-btn ir"><span>Close</span></a>
				<form action="#" class="padder">
					<div class="group">
						<label>Title</label>

							<div class="input-wrapper text-wrapper ">
		<div class="box-top-left"></div>
		<div class="box-top"></div>
		<div class="box-top-right"></div>
		<div class="box-body">
			<input type="text" value="" onclick="this.value=''" />
		</div>
		<div class="box-bottom-left"></div>
		<div class="box-bottom"></div>

		<div class="box-bottom-right"></div>
	</div>

						</div>
					<div class="group">
						<label>Tags <span class="instr">(E.g. accepted, rejected, pending)</span></label>
							<div class="input-wrapper text-wrapper ">
		<div class="box-top-left"></div>

		<div class="box-top"></div>
		<div class="box-top-right"></div>
		<div class="box-body">
			<input type="text" value="" onclick="this.value=''" />
		</div>
		<div class="box-bottom-left"></div>
		<div class="box-bottom"></div>
		<div class="box-bottom-right"></div>
	</div>

						</div>
					<div class="group choose-col">
						<a href="#" class="btn choose-files-btn ir"><span>Choose File(s)</span></a>
						<span class="filename">accepted.psd</span>
					</div>
					<div class="group submit-col">
						<input type="submit" class="btn" value="Add Task" />

					</div>
				</form>
			</div>

						</li>
					<li class="modal-trigger">
						<a class="create-todo" href="#">Add To-Do</a>
												<div id="add-todo-chat-modal" class="modal" style="">
							<a href="#" class="btn close-btn ir"><span>Close</span></a>

							<form action="#" class="padder">
								<div class="group">
									<label>To-Do Title</label>
										<div class="input-wrapper text-wrapper ">
		<div class="box-top-left"></div>
		<div class="box-top"></div>
		<div class="box-top-right"></div>
		<div class="box-body">

			<input type="text" value="" onclick="this.value=''" />
		</div>
		<div class="box-bottom-left"></div>
		<div class="box-bottom"></div>
		<div class="box-bottom-right"></div>
	</div>

									</div>
								<div class="group task-col">

									<label>Associated Task</label>
									<select>
										<option>Revamp the Nike.com Homepage</option>
										<option>Marketing Proposal</option>
										<option>Revamp the Nike.com Homepage</option>
									</select>
								</div>

								<div class="group date-col">
									<label>Due Date <span class="instr">(E.g. 01/15/2010)</span></label>
										<div class="input-wrapper text-wrapper ">
		<div class="box-top-left"></div>
		<div class="box-top"></div>
		<div class="box-top-right"></div>
		<div class="box-body">
			<input type="text" value="no date" onclick="this.value=''" />

		</div>
		<div class="box-bottom-left"></div>
		<div class="box-bottom"></div>
		<div class="box-bottom-right"></div>
	</div>

									</div>
								<div class="calendar-widget">
									<a href="#" class="btn ir calendar-btn"><span>Date picker</span></a>

									<div class="calendar-flyout">
										
						<div class="calendar f-lft">
							<div class="month-nav">
								<a class="icon icon-left ir" href="#"><span>&lsaquo;&lsaquo;</span></a>
								<a class="month-text" href="#">September</a>
								<a class="icon icon-right ir" href="#"><span>&rsaquo;&rsaquo;</span></a>
							</div>
							<table border="0" cellpadding="0" cellspacing="8">

								<thead>
									<tr>
										<th>S</th>
										<th>M</th>
										<th>T</th>
										<th>W</th>
										<th>T</th>

										<th>F</th>
										<th>S</th>
									</tr>
								</thead>
								<tbody>
									<tr>
										<td class="dim"><a href="#">29</a></td>

										<td class="dim"><a href="#">30</a></td>
										<td class="dim"><a href="#">31</a></td>
										<td><a href="#">1</a></td>
