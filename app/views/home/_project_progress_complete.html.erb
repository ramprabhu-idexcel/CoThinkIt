<%unless @progress_projects.empty?%>
  <div class="sidebar-box">
			<h4 class="box-top">In Progress</h4>
			<ul class="online-users">
			<% @progress_projects.each_with_index do |project,index| %>
				<% if project.owner && !project.owner.nil? %>
					<% @site_url="https://#{project.owner.site_address}.#{APP_CONFIG[:site][:name]}" %>
					<li class="online <%=index.odd? ? "alt" : ""%>"  id="project_<%=project.id%>"><a href="<%=@site_url+project_chat_index_path(project.url, project) %>"><%= project.name %></a></li>
				<% end %>
			<% end %>
				<!--<li class="newmessages alt"><a href="#">Apple</a></li>
				<li class="online"><a href="#">IBM</a></li>
				<li class="online alt"><a href="#">HP</a></li> -->
			</ul>
			<div class="box-bottom"></div>
    <%end%>
    <%unless @completed_projects.empty?%>
      <div class="sidebar-box">
        <div class="box-top">
          <h4>Recently completed</h4>
          <%if @all_completed_projects.count>3%><a class="h-action" href="<%=all_projects_path%>">More</a><%end%>
        </div>
        <ul class="online-users">
        <% @completed_projects.each do |project| %>
          <% if project.owner && !project.owner.nil? %>
            <% @site_url="https://#{project.owner.site_address}.#{APP_CONFIG[:site][:name]}" %>
            <li class="complete"><a href="<%= @site_url+project_chat_index_path(project.url, project) %>"><%= project.name %></a></li>
          <% end %>
        <% end %>
  <!--
          <li class="complete"><a href="#">Simple Station</a></li>
          <li class="complete"><a href="#">HurryUp.com</a></li> -->
        </ul>
        <div class="box-bottom"></div>
    <%end%>
		</div>